<?xml version="1.0" encoding="UTF-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Com.Latipium.Daemon</id>
    <version>{{ version }}</version>
    <title>Latipium Launcher Daemon</title>
    <authors>Latipium</authors>
    <description>The daemon the website will connect to in order to launch Latipium</description>
    <releaseNotes>
    </releaseNotes>
    <summary>The daemon the website will connect to in order to launch Latipium</summary>
    <language>en-US</language>
    <projectUrl>https://github.com/latipium/daemon</projectUrl>
    <iconUrl>https://latipium.com/favicon.ico</iconUrl>
    <licenseUrl>https://github.com/latipium/daemon/blob/master/LICENSE</licenseUrl>
    <copyright>Copyright (c) 2016 Latipium</copyright>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <dependencies>
      <dependency id="Com.Latipium.Daemon.Api" version="0.0.50.2" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" />
    </frameworkAssemblies>
    <tags>Latipium Development Non-Mod</tags>
  </metadata>
  <files>
    <file src="bin/{{ configuration }}/Com.Latipium.Daemon.exe" target="bin" />
  </files>
</package>
